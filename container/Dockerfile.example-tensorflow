FROM onnx-converter/examples-base:local

RUN uv pip install --python /opt/examples-venv/bin/python --editable ".[runtime]" && \
    uv pip install --python /opt/examples-venv/bin/python "tensorflow-cpu>=2.13.0" "tf2onnx>=1.16.0"

USER appuser:appgroup
ENTRYPOINT ["python3", "examples/tensorflow_example.py"]
